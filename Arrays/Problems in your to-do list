#include <iostream>
using namespace std;

int main() {

    int n;
    cin>>n;
    while(n--){
        int size,count = 0;
        cin>>size;
        int arr[size];
        for(int i=0;i<size;i++){
            cin>>arr[i];
            if(arr[i]>=1000){
                count++;
            }
        }
        cout<<count<<endl;
    }
	return 0;
}
