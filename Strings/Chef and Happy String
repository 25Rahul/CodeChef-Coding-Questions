#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	while(n--){
	    string s;
	    cin>>s;
	    int i=0;
	    for( i=0;i<s.length()-2;i++){
	        if((s[i] == 'a' || s[i] == 'e' ||s[i] == 'i' ||s[i] == 'o' ||s[i] == 'u')
	        && (s[i+1] =='a' ||  s[i+1] =='e' || s[i+1] =='i' || s[i+1] =='o' || s[i+1] =='u')
	        &&( s[i+2] =='a'  || s[i+2] =='i' || s[i+2] =='e' ||s[i+2] =='o' ||s[i+2] =='u')   ){
	            
	            cout << "HAPPY";
	          
	            break;
	        }
	        
	    
	    }
	     if(i==s.length()-2) cout << "SAD";
	    
  cout << "\n";	  
	}
	
	return 0;
}
