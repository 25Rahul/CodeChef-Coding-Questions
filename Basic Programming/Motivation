#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	while(n--){
	    int x,y;
	    cin>>x>>y;
	    int a,b;
	     cin>>a>>b;
	    for(int i=0;i<=y;i++){
	        if(a=1){
	        cout<<"1"<<endl;
	        }
	           else if(a<b){
	                cout<<b<<endl;
	            }
	            else{
	                cout<<a<<endl;
	            }
	        }
	    }
	return 0;
}
