#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	while(n--){
	    int a,b,x;
	    cin>>a>>b>>x;
	    if(a==b){
	        cout << "YES"<<"\n";
	    }else{
	        if(abs(a-b)%(2*x)==0)  cout << "YES"<<"\n";
	        else  cout << "NO"<<"\n";
	    }
	}
	return 0;
}
